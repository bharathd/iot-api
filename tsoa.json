{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
      "outputDirectory": "public",
      "specVersion": 3,
      "securityDefinitions": {
          "token": {
              "type": "http",
              "scheme": "bearer",
              "bearerFormat": "JWT"
          }
      }
  },
  "ignore": ["**/node_modules/**"]
}